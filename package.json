{
  "private": true,
  "type": "module",
  "name": "prisma-yoga-example",
  "scripts": {
    "prepare": "npm run generate",
    "generate": "npm run generate:schema && npm run generate:prisma && npm run generate:graphql",
    "generate:schema": "ts-node-esm scripts/extractSchema.ts",
    "generate:prisma": "prisma generate",
    "generate:graphql": "npm run codegen",
    "prestart": "tsc -p tsconfig.build.json",
    "start": "cross-env DATABASE_NAME=dev node built//server.js",
    "migrate:dev": "cross-env DATABASE_NAME=dev prisma migrate dev",
    "migrate:dev:reset": "cross-env DATABASE_NAME=dev prisma migrate reset",
    "migrate:test": "cross-env DATABASE_NAME=test prisma migrate dev",
    "migrate:test:reset": "cross-env DATABASE_NAME=test prisma migrate reset",
    "seed": "cross-env DATABASE_NAME=dev ts-node-esm scripts/seed.ts",
    "test": "cross-env DATABASE_NAME=test NODE_OPTIONS=--experimental-vm-modules jest",
    "compile": "tsc --noEmit",
    "format": "prettier . --write",
    "codegen": "graphql-codegen-esm --config codegen.yml"
  },
  "author": "Quramy",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.0",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-resolvers": "3.1.1",
    "@ngneat/falso": "^6.1.0",
    "@quramy/jest-prisma-node": "^1.3.0",
    "@quramy/prisma-fabbrica": "^1.0.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.8.0",
    "prisma": "^4.7.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "^4.7.0",
    "@types/jest": "^29.2.1",
    "dataloader": "^2.1.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.1.0",
    "jest": "^29.2.2",
    "jest-environment-node": "^29.2.2",
    "ts-node": "^10.9.1"
  }
}
