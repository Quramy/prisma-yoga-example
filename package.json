{
  "private": true,
  "type": "module",
  "name": "prisma-yoga-example",
  "scripts": {
    "prepare": "npm run generate",
    "generate": "npm run generate:schema && npm run generate:prisma && npm run generate:graphql",
    "generate:schema": "node --import=tsimp/import scripts/extractSchema.ts",
    "generate:prisma": "prisma generate",
    "generate:graphql": "npm run codegen",
    "dev": "cross-env DATABASE_NAME=dev node --watch --import=tsimp/import src/main.ts",
    "start": "cross-env DATABASE_NAME=dev node --import=tsimp/import src/main.ts",
    "migrate:dev": "cross-env DATABASE_NAME=dev prisma migrate dev",
    "migrate:dev:reset": "cross-env DATABASE_NAME=dev prisma migrate reset",
    "migrate:test": "cross-env DATABASE_NAME=test prisma migrate dev",
    "migrate:test:reset": "cross-env DATABASE_NAME=test prisma migrate reset",
    "seed": "cross-env DATABASE_NAME=dev node --import=tsimp/import scripts/seed.ts",
    "test": "cross-env DATABASE_NAME=test NODE_OPTIONS=--experimental-vm-modules jest",
    "compile": "tsc --noEmit",
    "format": "prettier . --write",
    "codegen": "graphql-codegen-esm --config codegen.ts"
  },
  "author": "Quramy",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/typescript": "4.1.0",
    "@graphql-codegen/typescript-resolvers": "4.3.0",
    "@ngneat/falso": "^7.0.0",
    "@quramy/jest-prisma-node": "^1.3.0",
    "@quramy/prisma-fabbrica": "^2.0.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.8.0",
    "prisma": "^5.0.0",
    "ts-jest": "^29.0.3",
    "tsimp": "^2.0.11",
    "typescript": "~5.5.0"
  },
  "dependencies": {
    "@graphql-tools/utils": "^10.5.4",
    "@prisma/client": "^5.0.0",
    "@types/jest": "^29.2.1",
    "dataloader": "^2.1.0",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.2",
    "graphql-yoga": "^5.0.0",
    "jest": "^29.2.2",
    "jest-environment-node": "^29.2.2"
  }
}
